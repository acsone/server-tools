<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record model="ir.ui.view" id="export_wizard_form">
        <field name="model">ir.exports.wizard</field>
        <field name="inherit_id" ref="base_ir_export.export_wizard_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="default_xml_export" />
            </xpath>
        </field>
    </record>
</odoo>
